<!DOCTYPE html>
<html>
<head>
    <%- include("./partial/head.ejs") %>

</head>
<body>
    <%- include("./partial/parent_nav.ejs") %>
    <div class="container bg-dark p-4 px-5">
        <div class="container text-center bg-white">
            <div class="row bg-secondary bg-opacity-50 "> 
                <h1 class="fw-bold">Courses</h1>
            </div>
            <div>
                Buttons of add and remove. Make course list as a checklist
            </div>
            <% cList.forEach((y)=>{ %> 
                <div class="row border border-bottom-2">
                    <a href="/CoursePage/<%= y.course_name %> " style="text-decoration: none;"   >
                   <h3><%= y.course_name %> </h3> 
                    </a>
                </div>
                <%}) %> 
        </div>
    </div>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/js/bootstrap.bundle.min.js" integrity="sha384-ka7Sk0Gln4gmtz2MlQnikT1wXgYsOg+OMhuP+IlRH9sENBO0LRn5q+8nbTov4+1p" crossorigin="anonymous"></script>
</body>
</html>
